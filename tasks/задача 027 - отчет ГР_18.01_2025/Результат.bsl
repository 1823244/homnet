// Назначение: Создание группы показателей ГР_18.01_2025
// Параметры: ВыводитьСообщения - Булево - Флаг вывода диагностических сообщений
Процедура СоздатьГруппуПоказателей(Знач ВыводитьСообщения = Ложь)

	#region initialization
	
	Идентификатор = "ГР_18.01_2025";
	НаименованиеГруппы = "ГР_18.01_2025";
	
	#endregion
	
	#region validation
	
	ГруппаСсылка = Справочники.хн_нфо_ПоказателиОтчета.НайтиПоНаименованию(НаименованиеГруппы,истина);
	
	#endregion
	
	#region group_creation
	
	Если НЕ ЗначениеЗаполнено(ГруппаСсылка) Тогда
		
		ГруппаОбъект = Справочники.хн_нфо_ПоказателиОтчета.СоздатьГруппу();
		ГруппаОбъект.Идентификатор = Идентификатор;
		ГруппаОбъект.Наименование = НаименованиеГруппы;
		ГруппаОбъект.Записать();
		
		Если ВыводитьСообщения Тогда
			Сообщить("<group_creation>Создана группа показателей: " + НаименованиеГруппы + "</group_creation>");
		КонецЕсли;
		
	Иначе
		
		Если ВыводитьСообщения Тогда
			Сообщить("<group_creation>Группа показателей уже существует: " + НаименованиеГруппы + "</group_creation>");
		КонецЕсли;
		
	КонецЕсли;
	
	#endregion

КонецПроцедуры

// Назначение: Пометка всех показателей группы на удаление
// Параметры: ВыводитьСообщения - Булево - Флаг вывода диагностических сообщений
Процедура ПометитьВсеПоказателиНаУдаление(Знач ВыводитьСообщения = Ложь)

	#region initialization
	
	ИмяГруппы = "ГР_18.01_2025";
	
	#endregion
	
	#region validation
	
	ГруппаСсылка = Справочники.хн_нфо_ПоказателиОтчета.НайтиПоНаименованию(ИмяГруппы, Истина);
	Если НЕ ЗначениеЗаполнено(ГруппаСсылка) Тогда
		
		Если ВыводитьСообщения Тогда
			Сообщить("<error_details>Ошибка: Группа " + ИмяГруппы + " не найдена. Невозможно пометить показатели на удаление.</error_details>");
		КонецЕсли;
		Возврат;
		
	КонецЕсли;
	
	#endregion
	
	#region data_processing
	
	Выборка = Справочники.хн_нфо_ПоказателиОтчета.Выбрать(ГруппаСсылка);
	Пока Выборка.Следующий() Цикл
		
		Если НЕ Выборка.ЭтоГруппа Тогда
			ОбъектПоказателя = Выборка.ПолучитьОбъект();
			ОбъектПоказателя.ПометкаУдаления = Истина;
			ОбъектПоказателя.Записать();
			Если ВыводитьСообщения Тогда
				Сообщить("<status_change>Показатель " + ОбъектПоказателя.Наименование + " помечен на удаление.</status_change>");
			КонецЕсли;
		КонецЕсли;
		
	КонецЦикла;
	
	Если ВыводитьСообщения Тогда
		Сообщить("<operation_end>Все показатели в группе " + ИмяГруппы + " помечены на удаление.</operation_end>");
	КонецЕсли;
	
	#endregion

КонецПроцедуры

